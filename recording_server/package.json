{
    "name": "node-typescript-boilerplate",
    "version": "0.0.0",
    "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
    "engines": {
        "node": ">= 14.16 <=18"
    },
    "dependencies": {
        "amqplib": "^0.10.3",
        "axios": "0.21.1",
        "body-parser": "1.19.0",
        "express": "4.17.1",
        "jsdom": "24.0.0",
        "node": "16.14.0",
        "puppeteer": "13.1.3",
        "puppeteer-extra": "3.2.3",
        "puppeteer-extra-plugin-stealth": "2.9.0",
        "ramda": "0.29.1",
        "redis": "4.6.7"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.1",
        "@types/body-parser": "^1.19.0",
        "@types/express": "^4.17.11",
        "@types/jsdom": "^21.1.6",
        "@types/node": "~14.14.45",
        "@types/ramda": "0.29.1",
        "@types/redis": "^4.0.10",
        "rimraf": "~3.0.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4"
    },
    "scripts": {
        "start": "node build/src/main.js",
        "start_extension_id": "node build/src/main.js get_extension_id",
        "clean": "rimraf coverage build tmp",
        "build": "echo '{\"buildDate\": \"'$(date -u +'%Y-%m-%dT%H:%M:%SZ')'\"}' > src/buildInfo.json && ./node_modules/typescript/bin/tsc --skipLibCheck -p tsconfig.release.json",
        "watch": "./node_modules/typescript/bin/tsc --skipLibCheck -w -p tsconfig.release.json",
        "watch-dev" : "source ../../.env && PROFILE=DEV ts-node-dev --respawn --transpile-only src/main.ts -p tsconfig.release.json"
    },
    "author": "Jakub Synowiec <github@jakubsynowiec.info>",
    "license": "Apache-2.0",
    "volta": {
        "node": "14.16.1"
    }
}
