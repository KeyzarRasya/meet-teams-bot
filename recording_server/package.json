{
    "name": "node-typescript-boilerplate",
    "version": "0.0.0",
    "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
    "engines": {
        "node": ">= 14.16 <=18"
    },
    "dependencies": {
        "@playwright/test": "^1.50.1",
        "amqplib": "^0.10.3",
        "async": "^3.2.1",
        "axios": "0.21.1",
        "express": "4.17.1",
        "fs": "^0.0.1-security",
        "jsdom": "24.0.0",
        "node": "16.14.0",
        "node-fetch": "^2.7.0",
        "path": "^0.12.7",
        "pino": "^8.19.0",
        "pino-caller": "^3.4.0",
        "puppeteer": "^24.2.1",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "ramda": "0.29.1",
        "redis": "4.6.7",
        "retry-axios": "^2.5.0",
        "tesseract.js": "^6.0.0",
        "tslib": "^2.8.1",
        "wav-encoder": "1.3.0",
        "ws": "8.18.0"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.1",
        "@types/async": "^3.2.7",
        "@types/body-parser": "^1.19.0",
        "@types/express": "^4.17.11",
        "@types/jest": "^29.5.14",
        "@types/jsdom": "^21.1.6",
        "@types/node": "~14.14.45",
        "@types/pino": "^7.0.5",
        "@types/ramda": "0.29.1",
        "@types/redis": "^4.0.10",
        "@types/wav-encoder": "1.3.3",
        "@types/ws": "8.5.12",
        "pino-pretty": "^10.3.1",
        "prettier": "3.3.3",
        "rimraf": "~3.0.2",
        "ts-jest": "^29.2.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4"
    },
    "scripts": {
        "start": "node build/src/main.js",
        "start_extension_id": "node build/src/main.js get_extension_id",
        "clean": "rimraf coverage build tmp",
        "build": "echo '{\"buildDate\": \"'$(date -u +'%Y-%m-%dT%H:%M:%SZ')'\"}' > src/buildInfo.json && ./node_modules/typescript/bin/tsc --skipLibCheck -p tsconfig.release.json",
        "watch": "./node_modules/typescript/bin/tsc --skipLibCheck -w -p tsconfig.release.json",
        "watch-dev": "PROFILE=DEV ts-node-dev --respawn --transpile-only src/main.ts -p tsconfig.release.json",
        "format": "prettier --log-level warn --write \"src/**/*.{jsx,js,ts}\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "author": "Jakub Synowiec <github@jakubsynowiec.info>",
    "license": "Apache-2.0",
    "volta": {
        "node": "14.16.1"
    }
}
